doctype html
html(lang='en')
  block head
      title= title
      link(href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,500,600,700,700i|Montserrat:300,400,500,600,700" rel="stylesheet")
      link(rel='stylesheet' href='css/all.css')
      link(rel='icon' type='image/x-icon' href='/images/favicon.new.ico')
      link(rel='stylesheet' href='/stylesheets/style.css')
      link(rel='stylesheet' href='/stylesheets/leaderboards.css' media='screen')
      link(rel='stylesheet' href='/stylesheets/media_queries.css')
      link(rel='stylesheet' href='https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css', integrity='sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T' crossorigin='anonymous')
      script(src="/js/norc.js")
      script(src="/js/mail.js")
      script(src="/js/dropdown.js")
      script(src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous")
      script(src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js" integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1" crossorigin="anonymous")
      script(src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js" integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM" crossorigin="anonymous")
      script (src="https://platform.twitter.com/widgets.js" charset="utf-8")
      meta(name='viewport' content='width=device-width, initial-scale=1.0')
      meta(charset='utf-8')
  body.d-flex.flex-column.h-100
    header
      nav.shadow.navbar.navbar-expand-lg.sticky-top.navbar-dark(style='background-color: crimson')
        a(class='navbar-brand' href='#')
          img(src='/images/Eclipse square logo.png' alt='Silver State High School Esports Logo' width='86' height='66')
        button(class='navbar-toggler' type='button' data-toggle='collapse' data-target='#navbarSupportedContent' aria-controls='navbarSupportedContent')
          span.navbar-toggler-icon
        div(class='collapse navbar-collapse' id='navbarSupportedContent')
          ul(class='navbar-nav mr-auto')
            if page === 'home'
              li(class='nav-item active')
                a(class='nav-link' href='#') Home
            else
              li(class='nav-item')
                a(class='nav-link' href='/') Home
            if page === 'gallery'
              li(class='nav-item active')
                a(class='nav-link' href='#') Gallery
            else
              li(class='nav-item')
                a(class='nav-link' href='/gallery') Gallery
            if page === 'leaugeinfo'
              li(class='nav-item active')
                a(class='nav-link' href='#') Club Info
            else
              li(class='nav-item')
                a(class='nav-link' href='/leagueinfo') Club Info
            if page === 'resources'
              li(class='nav-item active')
                a(class='nav-link' href='#') Resources
            else
              li(class='nav-item')
                a(class='nav-link' href='/resources') Resources
            //if page === 'smash'
              li(class='nav-item active')
                a(class='nav-link' href='#') Smash
            //else
              li(class='nav-item')
                a(class='nav-link' href='/smash') Smash
            //if page === 'league'
              li(class='nav-item active')
                a(class='nav-link' href='#') League
            //else
              li(class='nav-item')
                a(class='nav-link' href='/league') League
            //if page === 'mc'
              li(class='nav-item active')
                a(class='nav-link' href='#') Minecraft
            //else
              li(class='nav-item')
                a(class='nav-link' href='/mc') Minecraft
            //if page === 'fortnite'
              li(class='nav-item active')
                a(class='nav-link' href='#') Fornite
            //else
              li(class='nav-item')
                a(class='nav-link' href='/fortnite') Fortnite
            //if page === 'overwatch'
              li(class='nav-item active')
                a(class='nav-link' href='#') Overwatch
            //else
              li(class='nav-item')
                a(class='nav-link' href='/overwatch') Overwatch
            //if page === 'leaderboards'
              li(class='nav-item active')
                a(class='nav-link' href='#') Rankings
            //else
              li(class='nav-item')
                a(class='nav-link' href='/Leaderboards') Rankings
            //if page === 'others'
              li(class='nav-item active')
                a(class='nav-link' href='#') Other Games
            //else
              li(class='nav-item')
                a(class='nav-link' href='/others') Other Games
            //.dropdown
                li(class='nav-item')
                    button.dropbtn(onclick='myFunction()') Events
                    #myDropdown.dropdown-content
                        a(class='nav-link' href='/solo') Solo Events
                        a(class='nav-link' href='/team') Team Events
                        a(class='nav-link' href='/oneoff') One-Off Events

            //if page === 'leaderboards'
              li(class='nav-item active')
                a(class='nav-link' href='#') Power Rankings
            //else
              li(class='nav-item')
                a(class='nav-link' href='/rankings') Power Rankings
            if page === 'about'
              li(class='nav-item active')
                a(class='nav-link' href='#') About
            else
              li(class='nav-item')
                a(class='nav-link' href='/about') About
            if page === 'help'
              li(class='nav-item active')
                a(class='nav-link' href='#') Help
            else
              li(class='nav-item')
                a(class='nav-link' href='/help') Help



    block content
    footer.footer.py-3.mt-auto.container-fluid
      .row.justify-content-sm-center
        .col-sm-auto
          p Copyright &copy; 2019 Ethan Aldrich. All rights reserved.
